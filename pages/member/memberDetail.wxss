/* pages/member/memberDetail.wxss */
.head-wrap{
    padding: 20rpx 30rpx 0;
    background-color: #D9AB5E;
}
.head-wrap .frame1{
    display: flex;
    padding-bottom: 40rpx;
    border-bottom: 1rpx solid rgba(255,255,255,.3);
}
.avatar{
  width: 120rpx;
  height: 120rpx;  
  border-radius: 10rpx; 
}
.frame1-a{
    margin-left: 20rpx;
    margin-right: auto;  
    display: flex;
    flex: auto;
    flex-direction: column;
    justify-content: center;
}
.frame1-a .name{
    font-size: 28rpx;
    color: white;
    font-weight: 500;
}
.frame1-a .title{
    margin-top: 10rpx;
    font-size: 24rpx;
    color: white;
}
.frame2{
    position: relative;
    display: flex;
    height:150rpx;
    align-items: center;
}
.frame2 .story{
    padding: 10rpx;
    height: 24rpx;
    font-size: 24rpx;
    color: white;
    line-height: 1;
    border: 1rpx solid white;
    border-radius: 5rpx;
}
.frame2-a{
    display: flex;
    justify-content: space-between;
    margin-left: auto;
    width: 335rpx;
}
.frame2-a view{
    line-height: 1;
    text-align: center;
}
.frame2-a .number{
    font-size: 32rpx;
    color: white;
}
.frame2-a .text{
    margin-top: 10rpx;
    font-size: 24rpx;
    color: white;
}
.frame2-b{
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1000;
    display: flex;
    width: 100%;
    height: 20rpx;
    background-color: white;
}
.frame2-b-a{
    width: 48rpx;
    background-color: #D9AB5E;
    border-bottom-right-radius: 40rpx 20rpx;
}
.frame2-b-b{
    flex: auto;
    background-color: #D9AB5E;
    border-bottom-left-radius: 40rpx 20rpx;   
}
#story{
    background: white;
}
#story .st_wr{
    position: relative;
    padding-left:80rpx;
}
.left-line{
    position: absolute;
    left: 0;
    display: flex;
    justify-content: center;
    padding-top: 60rpx;
    box-sizing: border-box;
    width: 80rpx;
    height: 100%;
}
.left-line .line{
    height: 100%;
    border-left: 1rpx dashed #ccc;
}
.st_list{
    padding-top: 40rpx;
}
.st_list .timeline{
   position: relative;
    z-index: 1000;
    margin-left: -80rpx;
    margin-bottom: 20rpx;
    padding-left: 80rpx;
    font-size: 24rpx;
    color: #999;
    background-repeat: no-repeat;
    background-position: 30rpx center;
    background-size: 20rpx 20rpx;
    /*background-image: url(data:image/png;base64,); */
}
.st_list .timeline.tl_year{
   margin-bottom: 1rem;
    color: #111;
    /*background-image: url(data:image/png;base64,); */
}
.st_list .saying{
    margin:0 0 20rpx 0;
    padding-right:20rpx;
    width:620rpx;
    font-size:28rpx;
    line-height:48rpx;
    color:#333333;
}
.st_list .gallery{
    display: table;
    width: 650rpx;
    margin-right: 20rpx;
    padding-bottom: 40rpx;
    border-bottom: 1rpx solid #ccc;
}
.st_list .gallery:last-child{
    border-bottom: 0;
}
.st_list .gallery image{
    position: relative;
    float: left;
    margin-bottom: 20rpx;
    margin-right: 10rpx;
    width: 200rpx;
    height: 200rpx;
}